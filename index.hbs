{{!< default}}

<div class="gh-outer">
<main class="site-main gh-inner">

    <div class="post-feed gh-feed">
        {{> "pinned"}}
        {{#foreach posts }}
            {{^has tag="#pinned,#pinned8"}}
               {{#match @index "=" 0 }}
                   {{> "notlazy-loop"}}
               {{else}}
                   {{> "loop"}}
               {{/match}}
            {{/has}}
            {{#match @index "=" 5 }}
               {{> "pinned8"}}               
            {{/match}}
        {{/foreach}}
    </div>

</main>
</div>
